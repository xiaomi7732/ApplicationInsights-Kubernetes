{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/src/ApplicationInsights.Kubernetes.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "run-unit-tests",
            "command": "dotnet",
            "type": "process",
            "args": [
                "test",
                "${workspaceFolder}/tests/UnitTests/UnitTests.csproj"
            ]
        }
    ]
}